window/size/viewport_height=720
window/stretch/mode="viewport"
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg", "res://addons/godot_sparrow/plugin.cfg")

